#include<stdio.h>

int main() {
    int n1, n2, i, count = 0;

    printf("Enter the values of n1 and n2 : ");
    scanf("%d %d", &n1, &n2);

    for(i=n1; i<=n2; i++) {
        if(i%2 != 0) { 
            count++; 
            if(count != 3) { 
                printf("%d ", i);
            }
            else {
                count = 0; 
            }
        }
    }
}
